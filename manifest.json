{
  "manifest_version": 2,
  "name": "greenbeam-ad-tracking",
  "description": "Tracking CO2 contribution of ad trackers.",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "permissions": [
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "scripts": [
      "background_script.js",
      "store.js"
    ]
  },
  "options_ui": {
    "page": "options/index.html"
  }
}