{
  "manifest_version": 2,
  "name": "greenbeam-ad-tracking",
  "description": "Tracking CO2 contribution of ad trackers.",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "permissions": [
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "scripts": [
      "background_script.js",
      "store.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Greenbeam Ads",
    "default_popup": "popup/greenbeam_ads.html"
  },
  "content_scripts": [{
    "matches": [
      "*://*/*"
    ],
    "js": [
      "content_script.js"
    ],
    "css": [
      "content_script.css"
    ]
  }],
  "options_ui": {
    "page": "options/index.html"
  }
}